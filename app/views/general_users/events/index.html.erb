<% @event.each do |event| %>
<div>
  <%= link_to general_users_event_path(event.id) do %>
  <%= attachment_image_tag event, :event_image %>
  <% end %>
  <h3><%= attachment_image_tag event.farmer_user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %><%= event.farmer_user.name %></h3>
  <p>イベント名:<%= event.title %></p>
  <p>日時:<%= event.event_date %></p>
  <p>内容：<%= event.introduction %></p>
  <p>参加費：<%= event.price %>円</p>


</div>
<% end %>